<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.fmp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FMP</name>
    <description>
        Family Media Player - Satellite.
    </description>
    <author email="theerm@gmail.com" href="https://github.com/the7erm/fmp-pg">
        Family Media Player
    </author>
    <content src="index.html" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" /><!-- Note that needs to be inside the android config -->
        <preference name="android-manifest/@android:installLocation" value="preferExternal" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-networking-bluetooth" spec="~1.0.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.7.2" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.4" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.7.0" />
    <plugin name="cordova-plugin-media" spec="~2.4.1" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
</widget>
